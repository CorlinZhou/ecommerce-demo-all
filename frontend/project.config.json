{
  "description": "WeChat Mini Program Project Configuration",
  "packOptions": {
    "ignore": [
      {
        "type": "folder",
        "value": "node_modules"
      },
      {
        "type": "folder",
        "value": ".git"
      },
      {
        "type": "file",
        "value": ".gitignore"
      },
      {
        "type": "file",
        "value": ".env.*"
      },
      {
        "type": "file",
        "value": "package.json"
      }
    ]
  },
  "setting": {
    "urlCheck": true,
    "es6": true,
    "enhance": true,
    "postcss": true,
    "preloadBackgroundData": false,
    "minified": true,
    "newFeature": true,
    "coverView": true,
    "nodeModules": false,
    "autoAudits": false,
    "showShadowRootInWxmlPanel": true,
    "scopeDataCheck": false,
    "uglifyFileName": false,
    "checkInvalidKey": true,
    "checkSiteMap": true,
    "uploadWithSourceMap": true,
    "compileHotReLoad": false,
    "useMultiFrameRuntime": true,
    "useApiHook": true,
    "useApiHostProcess": true,
    "showStatusBar": true,
    "enableEngineNative": false,
    "packageJsonPath": "package.json",
    "componentFramework": "glass-easel",
    "useIsolateContext": true
  },
  "compileType": "miniprogram",
  "libVersion": "2.19.4",
  "appid": "touristappid",
  "projectname": "elc-ecommerce-demo",
  "debugOptions": {
    "hidedInDevtools": []
  },
  "isGameTourist": false,
  "gameType": "",
  "condition": {
    "search": {
      "list": []
    },
    "conversation": {
      "list": []
    },
    "plugin": {
      "list": []
    },
    "game": {
      "list": []
    },
    "miniprogram": {
      "list": [],
      "enable-mp-component": true,
      "useCompilerModules": true
    }
  }
}
